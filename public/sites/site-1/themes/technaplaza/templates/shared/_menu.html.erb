<% if menu.present? %>
	<ul class="<%= menu.options[:class] %>">
		<% menu.items.each do |item| %>
			<li id="<%= item.id %>" class="<%= item.options[:class] %>">
				<%= item.caption %>
				<%= render :partial => item.partial, :object => item if item.is_a?(Menu::Base) %>
			</li>
		<% end %>
	</ul>
<% end %>