<% if @photo && @photo.errors && @photo.errors.size > 0 %>
	<%= error_messages_for(:photo) %>
<% elsif !request.xhr? %>
	<script type="text/javascript" charset="utf-8">
		Ext.onReady(function(){
			window.location = '<%= admin_photos_path(@site, @section) %>';
		});
	</script>
<% else %>
	CRUD_OK
<% end %>